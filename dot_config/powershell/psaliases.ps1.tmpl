{{- range .aliases }}
  {{- if not (and (hasKey . "bash_only") .bash_only) }}
    {{- if not (and (hasKey . "fish_only") .fish_only) }}
      {{- if hasKey . "pwsh_command" }}
Set-Alias {{ .alias }} {{ .pwsh_command }}
      {{- else }}
Set-Alias {{ .alias }} {{ .command }}
      {{- end }}
    {{- end }}
  {{- end }}
{{- end }}
